\par }\pard\plain \ql \fi360\li360\ri0\widctlpar\aspalpha\aspnum\faauto\adjustright\rin0\lin360\itap0 \fs24\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {\f37\fs20\insrsid5579329 JDBC
\par Informix
\par {\listtext\pard\plain\s2 \b\fs20\lang1033\langfe1033\loch\af37\hich\af37\dbch\af34\langnp1033\langfenp1033\insrsid5579329 \hich\af37\dbch\af34\loch\f37 1.4\tab}}\pard\plain \s2\ql \li0\ri0\sb120\sa60\sl240\slmult0\keepn\nowidctlpar
