<?xml version="1.0"?>
<CMConfig>
	<!-- config for Log wrapper -->
	<Config name="com.topcoder.util.log">
		<!-- Logging implementation class -->
		<Property name="logClass">
			<Value>com.topcoder.util.log.basic.BasicLog</Value>
		</Property>
		<Property name="factoryClass">
			<Value>com.topcoder.util.log.basic.BasicLogFactory</Value>
		</Property>
		
		<!-- Additional property file required by the logging implementation -->
		<Property name="basic.log.target">
			<!--<Value>System.out</Value>-->
			<Value>System.out</Value>
		</Property>
	</Config>
	
</CMConfig>