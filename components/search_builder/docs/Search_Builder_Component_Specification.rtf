\par }\pard \qj \fi720\li960\ri0\nowidctlpar\faauto\rin0\lin960\itap0 {\f2\fs16\cf1\insrsid2841421 if ((validators == null) || (alias == null)) \{
\par             throw new NullPointerException(
\par                 "The param should not be null to check Valid");
\par         \}
\par 
\par         for (int i = 0; i < filters.size(); i++) \{
\par             Filter tmp = (Filter) filters.get(i);
\par 
\par             ValidationResult v = tmp.isValid(validators, alias);
\par 
\par             //success then continue
\par             if (v.isValid()) \{
\par                 continue;
\par             \}
\par 
\par             //if fail the return the fail result
\par             return ValidationResult.createInvalidResult("fails",
\par                 v.getFailedFilter());
\par         \}
\par 
\par         //valid success
\par         return ValidationResult.createValidResult();
\par }\pard \ql \li1440\ri0\nowidctlpar\faauto\rin0\lin1440\itap0 {\fs20\insrsid2841421 
