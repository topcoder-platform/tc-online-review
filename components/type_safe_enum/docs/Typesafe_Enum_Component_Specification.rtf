\par }{\f2\fs18\cf17 \hich\af2\dbch\af17\loch\f2         MyBoo\hich\af2\dbch\af17\loch\f2 l mybool = MyBool.FALSE;
\par \hich\af2\dbch\af17\loch\f2         String enumConstantName = mybool.getEnumName();
\par \hich\af2\dbch\af17\loch\f2         assertTrue("enum constant name must be 'FALSE'", enumConstantName.equals("FALSE"));}{\f2\fs18\cf11 
