\par \hich\af2\dbch\af17\loch\f2     private TypeEnum(String name) \{
\par \hich\af2\dbch\af17\loch\f2         // Introduced in version 1.1, this is required to support internal subclassing.
\par \hich\af2\dbch\af17\loch\f2         super(TypeEnum.class);
\par \hich\af2\dbch\af17\loch\f2         if (name == null) \{
\par \hich\af2\dbch\af17\loch\f2             throw new IllegalArgumentException("name shouldn't be null.");
\par \hich\af2\dbch\af17\loch\f2         \}
