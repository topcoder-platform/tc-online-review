                    </Property>
                    <Property name="submission_status_id">
                        <Value>submission.submission_status_id</Value>
                    </Property>
                    <Property name="submission_type_id">
                        <Value>submission.submission_type_id</Value>
                    </Property>
                    <Property name="project_id">
