<?xml version="1.0"?>
<!-- A template which defines a set of project phases -->
<Template name="Updated Component Testing">
    <!-- An enumeration of project phase types defined in this template -->
    <PhaseTypes>
        <PhaseType id="registration" typeId="1" typeName="Registration"/>
        <PhaseType id="submission" typeId="2" typeName="Submission"/>
        <PhaseType id="screening" typeId="3" typeName="Screening"/>
        <PhaseType id="appeals_response" typeId="6" typeName="Appeals Response"/>
        <PhaseType id="aggregation" typeId="7" typeName="Aggregation"/>
        <PhaseType id="final_fixes" typeId="9" typeName="Final Fix"/>
        <PhaseType id="final_review" typeId="10" typeName="Final Review"/>
        <PhaseType id="approval" typeId="11" typeName="Approval"/>
        <PhaseType id="secondary_reviewer_review" typeId="15" typeName="Secondary Reviewer Review"/>
        <PhaseType id="primary_review_evaluation" typeId="16" typeName="Primary Review Evaluation"/>
        <PhaseType id="new_appeals" typeId="17" typeName="New Appeals"/>
        <PhaseType id="primary_review_appeals_response" typeId="19" typeName="Primary Review Appeals Response"/>
    </PhaseTypes>
    <!-- A set of project phases defined in this template. -->
    <Phases>
        <!-- A definition of a single phase. -->
        <Phase id="registrationPhase" length="86400000" type="registration" phaseId="1"/>

        <Phase id="submissionPhase" length="259200000" type="submission" phaseId="3">
            <Dependency id="registrationPhase" isDependencyStart="true" isDependentStart="true" lagTime="300000"/>
        </Phase>

        <Phase id="screeningPhase" length="7200000" type="screening" phaseId="4">
            <Dependency id="submissionPhase" isDependencyStart="false" isDependentStart="true" lagTime="0"/>
        </Phase>

        <Phase id="reviewPhase" length="172800000" type="secondary_reviewer_review" phaseId="5">
            <!-- "isDependencyStart" is optional attribute(default to "false"), here it is omitted -->
            <Dependency id="screeningPhase" isDependencyStart="false" isDependentStart="true" lagTime="0"/>
        </Phase>

        <Phase id="primaryReviewEvaluation" length="86400000" type="primary_review_evaluation" phaseId="6">
            <!-- "isDependentStart" is optional attribute(default to "true"), here it is omitted -->
            <Dependency id="reviewPhase" isDependencyStart="false" isDependentStart="true" lagTime="0"/>
        </Phase>

        <Phase id="newAppeals" length="86400000" type="new_appeals" phaseId="7">
            <!-- "isDependentStart" is optional attribute(default to "true"), here it is omitted -->
            <Dependency id="primaryReviewEvaluation" isDependencyStart="false" isDependentStart="true" lagTime="0"/>
        </Phase>

        <Phase id="primaryReviewAppealsResponse" length="43200000" type="primary_review_appeals_response" phaseId="8">
            <!-- "lagTime" is optional attribute(default to "0"), here it is omitted -->
            <Dependency id="newAppeals" isDependencyStart="false" isDependentStart="true"/>
        </Phase>

        <Phase id="aggregationPhase" length="18000000" type="aggregation" phaseId="9">
            <!-- all attributes except "id" are omitted -->
            <Dependency id="primaryReviewAppealsResponse"/>
        </Phase>

        <Phase id="finalFixesPhase" length="64800000" type="final_fixes" phaseId="11">
            <Dependency id="aggregationPhase" isDependencyStart="false" isDependentStart="true" lagTime="0"/>
        </Phase>

        <Phase id="finalReviewPhase" length="21600000" type="final_review" phaseId="12">
            <Dependency id="finalFixesPhase" isDependencyStart="false" isDependentStart="true" lagTime="0"/>
        </Phase>

        <Phase id="approvalPhase" length="43200000" type="approval" phaseId="13">
            <Dependency id="finalReviewPhase" isDependencyStart="false" isDependentStart="true" lagTime="0"/>
        </Phase>
    </Phases>
</Template>
