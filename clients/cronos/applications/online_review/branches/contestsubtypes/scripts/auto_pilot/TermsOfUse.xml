<CMConfig>
    <Config name="com.cronos.termsofuse.dao.TermsOfUseDao">
        <Property name="dbConnectionFactoryConfig">
            <Property name="com.topcoder.db.connectionfactory.DBConnectionFactoryImpl">
                <Property name="connections">
                    <Property name="default">
                        <Value>InformixJDBCConnection</Value>
                    </Property>
                    <Property name="InformixJDBCConnection">
                        <Property name="producer">
                            <Value>com.topcoder.db.connectionfactory.producers.JDBCConnectionProducer</Value>
                        </Property>
                        <Property name="parameters">
                            <Property name="jdbc_driver">
                                <Value>com.informix.jdbc.IfxDriver</Value>
                            </Property>
                            <Property name="jdbc_url">
                                <Value>jdbc:informix-sqli://@database.server@:@database.port@/common_oltp:informixserver=@database.INFORMIXSERVER@</Value>
                            </Property>
                            <Property name="user">
                                <Value>@database.username@</Value>
                            </Property>
                            <Property name="password">
                                <Value>@database.password@</Value>
                            </Property>
                        </Property>
                    </Property>
                </Property>
            </Property>
        </Property>
        <Property name="loggerName">
            <Value>com.cronos.termsofuse.dao.impl.TermsOfUseDaoImpl</Value>
        </Property>
