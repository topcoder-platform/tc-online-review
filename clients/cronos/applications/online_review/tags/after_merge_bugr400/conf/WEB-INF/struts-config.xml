            parameter="method" scope="request"
            name="fileUploadForm"
            input="/jsp/uploadFinalFix.jsp">
            <forward name="displayPage" path="/jsp/uploadFinalFix.jsp" redirect="false" />
            <forward name="success" path="/actions/ViewProjectDetails.do?method=viewProjectDetails" redirect="true" />
        </action>

        <!-- "Download Final Fix" action -->
        <action path="/actions/DownloadFinalFix" unknown="false"
            type="com.cronos.onlinereview.actions.ProjectDetailsActions"
            parameter="method" scope="request"
            input="/jsp/viewProjectDetails.jsp">
        </action>

        <!-- "Upload Test Case" action -->
        <action path="/actions/UploadTestCase" unknown="false"
            type="com.cronos.onlinereview.actions.ProjectDetailsActions"
            parameter="method" scope="request"
            name="fileUploadForm"
            input="/jsp/uploadTestCase.jsp">
            <forward name="displayPage" path="/jsp/uploadTestCase.jsp" redirect="false" />
            <forward name="success" path="/actions/ViewProjectDetails.do?method=viewProjectDetails" redirect="true" />
        </action>

        <!-- "Download Test Case" action -->
        <action path="/actions/DownloadTestCase" unknown="false"
            type="com.cronos.onlinereview.actions.ProjectDetailsActions"
            parameter="method" scope="request"
            input="/jsp/viewProjectDetails.jsp">
        </action>

        <!-- "Download Document" action -->
        <action path="/actions/DownloadDocument" unknown="false"
            type="com.cronos.onlinereview.actions.ProjectDetailsActions"
            parameter="method" scope="request">
        </action>

        <!-- "Delete Submission" action -->
        <action path="/actions/DeleteSubmission" unknown="false"
            type="com.cronos.onlinereview.actions.ProjectDetailsActions"
            parameter="method" scope="request"
            input="/jsp/confirmDeleteSubmission.jsp">
            <forward name="displayPage" path="/jsp/confirmDeleteSubmission.jsp" redirect="false"/>
