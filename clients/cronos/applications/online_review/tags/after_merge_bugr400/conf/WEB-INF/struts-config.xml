            input="/jsp/viewProjectDetails.jsp">
            <forward name="success" path="/jsp/editReview.jsp" redirect="false"/>
            <forward name="edit" path="/actions/EditReview.do?method=editReview" redirect="true"/>
        </action>

        <!-- "Edit Review" action -->
        <action path="/actions/EditReview" unknown="false"
